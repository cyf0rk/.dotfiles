#!/usr/bin/env bash
alias vim="$VIM"
alias awrc="cd $AWESOME_DIR && $EDITOR ."
alias vimrc="cd $NVIM_DIR && $EDITOR ."
alias devw="cd $HOME/work/development"
alias devp="cd $HOME/personal/development"
alias todow="cd $HOME/vaults/work && $EDITOR todo.md"
alias todop="cd $HOME/vaults/personal && $EDITOR todo.md"
